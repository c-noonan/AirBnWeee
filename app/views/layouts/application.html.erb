<!DOCTYPE html>
<html>
  <head>
    <title>Airbnweee</title>

    <%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=AIzaSyDoypKLh-YO-sasOJ8S-GKZ2BMXI9D_Fmw" %>

    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>

    <% if current_user %>
      <script>
        window.currentUser = {
          'id': <%= current_user.id %>,
          'username': "<%= current_user.username %>"
        };
      </script>
    <% end %>

    <link href="https://fonts.googleapis.com/css?family=Damion|Lobster" rel="stylesheet">

  </head>

  <body>

    <%= yield %>

  </body>
</html>
